<div class="field">
  <%= f.attribute_fields do |a| %>
      <%= a.attribute_select include_blank: false %>
  <% end %>
  <%= f.predicate_select  %>
  <%= f.value_fields do |v| %>
      <%= v.text_field :value, placeholder: "Value" %>
  <% end %>
  <%= link_to "x", '#', class: "remove_fields" %>
</div>
