<%= form_tag bulk_import_admin_lot_numbers_path, method: :post, :html => {:class => 'form-horizontal'} do |f| %>
    <div class="modal-body">

      <div class="row">
        <div class="col-xs-6">
          <b>From:</b> <%= text_field_tag :start, params[:start], :class => 'text_field' %>
        </div>
        <div class="col-xs-6">
          <b>To:</b> <%= text_field_tag :end, params[:end], :class => 'text_field' %>
        </div>
      </div>
    </div>

    <div class="modal-footer">
      <%= link_to "Cancel", admin_lot_numbers_path, :class => "pull-left", :data => {:dismiss => 'modal'} %>
      <%= submit_tag "Save", :class => 'btn btn-primary', disable_with: "Processing..." %>
    </div>
<% end %>

