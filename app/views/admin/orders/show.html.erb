<%- model_class = Order -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:number) %>:</strong></dt>
  <dd><%= @order.number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:customer_id) %>:</strong></dt>
  <dd><%= @order.customer_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:completed_at) %>:</strong></dt>
  <dd><%= @order.completed_at %></dd>
</dl>

