<%= form_for @catalog, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :lot_number_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :lot_number_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :plant_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :plant_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :qty_in_bank, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :qty_in_bank, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :qty_on_consignment, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :qty_on_consignment, :class => 'number_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                catalogs_path, :class => 'btn' %>
  </div>
<% end %>
