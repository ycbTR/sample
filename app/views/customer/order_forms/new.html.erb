<div class="col-lg-12">
  <%= form_for @current_order, url: complete_customer_order_forms_path, method: :put, html: {class: "form-horizontal"} do |f| %>
      <%= f.hidden_field :type, value: "OrderForm::#{(params[:type] || 'seeding').titleize}" %>
      <%= render partial: (params[:type] || 'seeding'), locals: {f: f} %>
  <% end %>
</div>